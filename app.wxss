.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
}

.container-full {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.tips-pop-mask{
  position: fixed;
  left: 0;
  top: 0;
  z-index: 8888;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
}
